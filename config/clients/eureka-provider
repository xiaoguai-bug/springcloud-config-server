server:
  port: 8088

eureka:
  client:
    service-url:
      #服务端的地址，客户端将向该地址注册自己的信息
      defaultZone: http://localhost:8761/eureka/
spring:
  application:
    #设置当前客户端的连接名
    name: eureka-provider
  datasource:
    url: jdbc:mysql://192.168.50.9:3306/mall?serverTimezone=UTC&useSSL=false&useUnicode=true&characterEncoding=UTF-8
    username: root
    password: root
    driver-class-name: com.mysql.cj.jdbc.Driver
